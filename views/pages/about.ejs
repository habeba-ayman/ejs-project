<!DOCTYPE html>
<html lang="en">
  <head>
    <%- include('../partials/head'); %>
  </head>
  <body class="container">
    <header><%- include('../partials/header'); %></header>

    <main>
      <div class="row">
        <div class="col-sm-8">
          <div class="jumbotron">
            <h1>About Page</h1>
            <p>Welcome to my project using EJS</p>

            <h2>information about me</h2>
            <p><%= myJob %></p>

            <ul>
              <% myInformation.forEach(function(myInformation) { %>
              <li>
                <strong><%= myInformation.name %></strong>
                representing <%= myInformation.organization %>, born <%=
                myInformation.birth_year %>
              </li>
              <% }); %>
            </ul>
          </div>
        </div>

        <div class="col-sm-4">
          <div class="well">
            <h3>Look I'm A Sidebar!</h3>
          </div>
        </div>
      </div>
    </main>

    <footer><%- include('../partials/footer'); %></footer>
  </body>
</html>
